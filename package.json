{
    "name": "js-quick-log",
    "displayName": "js quick log",
    "description": "Help insert and remove console.(*) statements. (based on a fork of javascript console utils)",
    "version": "0.1.2",
    "publisher": "Void0",
    "repository": "https://github.com/fml-song/vscode-js-console-utils",
    "icon": "icon.png",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "console.log",
        "javascript",
        "debugging",
        "deep copy",
        "log"
    ],
    "activationEvents": [],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.insertLogStatement",
                "title": "Insert Log Statement"
            },
            {
                "command": "extension.deleteAllLogStatements",
                "title": "Delete all console.log statements"
            },
            {
                "command": "extension.insertJsonLogStatement",
                "title": "Insert JSON Deep Copy Log Statement"
            }
        ],
        "keybindings": [
            {
                "command": "extension.insertLogStatement",
                "key": "shift+ctrl+l",
                "mac": "shift+cmd+l",
                "when": "editorTextFocus"
            },
            {
                "command": "extension.deleteAllLogStatements",
                "key": "shift+ctrl+d",
                "mac": "shift+cmd+d"
            },
            {
                "command": "extension.insertJsonLogStatement",
                "key": "shift+ctrl+alt+l",
                "mac": "shift+cmd+alt+l",
                "when": "editorTextFocus"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "eslint": "^3.6.0",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}